{
  "name": "mimmi20/laminasviewrenderer-bootstrap-form",
  "description": "Provides a View Renderer for Laminas Forms to use with Bootstrap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "laminas/laminas-form": "^3.20.1",
    "laminas/laminas-inputfilter": "^2.30.1",
    "laminas/laminas-view": "^2.35.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11 || ^0.28.1 || ^0.29.0",
    "laminas/laminas-filter": "^2.37.0",
    "laminas/laminas-i18n": "^2.28.0",
    "laminas/laminas-modulemanager": "^2.16.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "laminas/laminas-validator": "^2.64.1",
    "mimmi20/coding-standard": "^5.2.41",
    "mimmi20/laminas-form-element-group": "^3.0.9",
    "mimmi20/laminas-form-element-links": "^2.0.8",
    "mimmi20/laminas-form-element-paragraph": "^2.0.8",
    "nikic/php-parser": "^4.19.1 || ^5.0.2",
    "phpstan/extension-installer": "^1.4.2",
    "phpstan/phpstan": "^1.12.0",
    "phpstan/phpstan-deprecation-rules": "^1.2.0",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.27",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^1.2.4",
    "rector/type-perfect": "^0.1.8",
    "symplify/phpstan-rules": "^13.0.1",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^0.3.1",
    "tomasvotruba/unused-public": "^0.3.10"
  },
  "suggest": {
    "laminas/laminas-i18n": "Required to use the Translator",
    "laminas/laminas-servicemanager": "Required to use the navigation factories",
    "mimmi20/laminas-form-element-group": "Required to render fieldsets just for styling",
    "mimmi20/laminas-form-element-links": "Required to render links inside a form",
    "mimmi20/laminas-form-element-paragraph": "Required to render paragraphs inside a form"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\BootstrapForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\BootstrapForm\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\BootstrapForm",
      "config-provider": "Mimmi20\\LaminasView\\BootstrapForm\\ConfigProvider"
    }
  }
}
