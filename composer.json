{
  "name": "mimmi20/laminasviewrenderer-bootstrap-form",
  "description": "Provides a View Renderer for Laminas Forms to use with Bootstrap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-form"
  },
  "require": {
    "php": "^8.1.0",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "laminas/laminas-escaper": "^2.13.0",
    "laminas/laminas-form": "^3.13.1",
    "laminas/laminas-json": "^3.6.0",
    "laminas/laminas-view": "^2.31.0",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^2.0.4"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.6",
    "laminas/laminas-i18n": "^2.23.1",
    "laminas/laminas-modulemanager": "^2.14.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "mimmi20/coding-standard": "^5.1.23",
    "mimmi20/laminas-form-element-group": "^3.0.5",
    "mimmi20/laminas-form-element-links": "^2.0.4",
    "mimmi20/laminas-form-element-paragraph": "^2.0.4",
    "nikic/php-parser": "^4.17.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.40",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpunit/phpunit": "^10.4.2",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^0.18.6"
  },
  "suggest": {
    "laminas/laminas-i18n": "Required to use the Translator",
    "laminas/laminas-servicemanager": "Required to use the navigation factories",
    "mimmi20/laminas-form-element-group": "Required to render fieldsets just for styling",
    "mimmi20/laminas-form-element-links": "Required to render links inside a form",
    "mimmi20/laminas-form-element-paragraph": "Required to render paragraphs inside a form"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\BootstrapForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\BootstrapForm\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "laminas/laminas-dependency-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\BootstrapForm",
      "config-provider": "Mimmi20\\LaminasView\\BootstrapForm\\ConfigProvider"
    }
  }
}
